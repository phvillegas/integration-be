{
  "name": "integration-be",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Pedro Hern√°ndez",
  "description": "Backend for integration to RDR",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "start": "node dist/server.js",
    "db:push": "drizzle-kit push",
    "dev": "tsx watch src/server.ts",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "format:check": "prettier --check 'src/**/*.{ts,js}'",
    "prepare": "husky"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.87.0",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.45.0",
    "express": "5.2.1",
    "helmet": "8.1.0",
    "pino": "10.1.0",
    "pino-http": "11.0.0",
    "postgres": "3.4.7",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/node": "24.10.1",
    "@types/pg": "8.15.6",
    "@types/supertest": "6.0.3",
    "drizzle-kit": "0.31.8",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "pino-pretty": "13.1.3",
    "prettier": "3.7.4",
    "supertest": "7.1.4",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "vitest": "4.0.15"
  }
}
